# Database
DB_CONNECTION_URI=your_db_url_for_example:mongodb://localhost:27017/inotes

# JWT Secret for local authentication
JWT_SECRET=your-secret-key-here-please-change-this-in-production

# Salt rounds for bcrypt
SALT=10

# Server Port
PORT=8000

# API Base URL
BASE_API_URL=/api

# Auth0 Configuration
AUTH0_DOMAIN=your-auth0-domain.auth0.com
AUTH0_CLIENT_ID=your-auth0-client-id
AUTH0_CLIENT_SECRET=your-auth0-client-secret
AUTH0_AUDIENCE=https://your-auth0-domain.auth0.com/api/v2/
